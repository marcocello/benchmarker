{
    "name": "Benchmarker Development Container",
    "dockerComposeFile": "docker-compose.yaml",
    "service": "benchmarker",
    "workspaceFolder": "/opt/benchmarker",

	"remoteEnv": {
		"LOCAL_WORKSPACE_FOLDER": "${localWorkspaceFolder}"
	},

	"customizations": {
		"vscode": {		
			"extensions": [
				"ms-azuretools.vscode-docker",
				"ms-python.python",
				"ms-python.debugpy"
			]
		}
	}
}